<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.techstar.dmis.entity">
	<class name="StdConsumer" table="DMIS_ZD_Std_consumer" dynamic-insert="true" dynamic-update="true" optimistic-lock="version">
    <id name="id" type="string">
      <column name="id" sql-type="varchar(40)" not-null="true"/>
      <generator class="assigned"/>
    </id>

 	<version name="version" column="VERSION" unsaved-value="undefined"/>
  
   <property name="consumername" type="string"  not-null="false">
    <column name="consumername"  
		length="80"               
    />
</property>	
    <property name="shortname" type="string"  not-null="false">
    <column name="shortname"  
		length="80"               
    />
</property>	
    <property name="grade" type="string"  not-null="false">
    <column name="grade"  
		length="80"               
    />
</property>	
    <property name="consumer" type="string"  not-null="false">
    <column name="consumer"  
		length="80"               
    />
</property>	
    <property name="telephon" type="string"  not-null="false">
    <column name="telephon"  
		length="80"               
    />
</property>	
    <property name="address" type="string"  not-null="false">
    <column name="address"  
		length="80"               
    />
</property>	
    <property name="zipcode" type="string"  not-null="false">
    <column name="zipcode"  
		length="80"               
    />
</property>	
    <property name="manageunitno" type="string"  not-null="false">
    <column name="manageunitno"  
		length="80"               
    />
</property>	
    <property name="dispathunitno" type="string"  not-null="false">
    <column name="dispathunitno"  
		length="80"               
    />
</property>	
  
</class>

</hibernate-mapping>

