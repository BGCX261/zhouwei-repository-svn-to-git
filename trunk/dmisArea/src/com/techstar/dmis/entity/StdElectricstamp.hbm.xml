<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.techstar.dmis.entity">
	<class name="StdElectricstamp" table="DMIS_ZD_std_electricstamp" dynamic-insert="true" dynamic-update="true" optimistic-lock="version">
    <id name="id" type="string">
      <column name="id" sql-type="varchar(40)" not-null="true"/>
      <generator class="assigned"/>
    </id>

 	<version name="version" column="VERSION" unsaved-value="undefined"/>
  
   <property name="fstampname" type="string"  not-null="false">
    <column name="fstampname"  
		length="40"               
    />
</property>	
    <property name="fgraph" type="org.springframework.orm.hibernate3.support.BlobByteArrayType"  not-null="false">
    <column name="fgraph"  
		               
    />
</property>	
    <property name="ftype" type="string"  not-null="false">
    <column name="ftype"  
		length="40"               
    />
</property>	
  
</class>

</hibernate-mapping>

