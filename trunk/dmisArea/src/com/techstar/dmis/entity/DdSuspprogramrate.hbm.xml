<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.techstar.dmis.entity">
	<class name="DdSuspprogramrate" table="DMIS_DD_SuspProgramRate" dynamic-insert="true" dynamic-update="true" optimistic-lock="version">
    <id name="fid" type="string">
      <column name="fid" sql-type="varchar(40)" not-null="true"/>
      <generator class="assigned"/>
    </id>

 	<version name="version" column="VERSION" unsaved-value="undefined"/>
  
   <property name="fannual" type="integer"  not-null="false">
    <column name="fannual"  
		               
    />
</property>	
    <property name="fdispatchunitname" type="string"  not-null="false">
    <column name="fdispatchunitname"  
		length="150"               
    />
</property>	
    <property name="floadrate" type="integer"  not-null="false">
    <column name="floadrate"  
		               
    />
</property>	
    <property name="fdispatchunitid" type="string"  not-null="false">
    <column name="fdispatchunitid"  
		length="40"               
    />
</property>	
  
</class>

</hibernate-mapping>

