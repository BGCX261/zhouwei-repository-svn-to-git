<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.techstar.dmis.entity">
	<class name="StdDispatchunit" table="DMIS_ZD_Std_DispatchUnit" dynamic-insert="true" dynamic-update="true" optimistic-lock="version">
    <id name="id" type="string">
      <column name="id" sql-type="varchar(40)" not-null="true"/>
      <generator class="assigned"/>
    </id>

 	<version name="version" column="VERSION" unsaved-value="undefined"/>
  
   <property name="unitname" type="string"  not-null="false">
    <column name="unitname"  
		length="80"               
    />
</property>	
    <property name="unitarea" type="string"  not-null="false">
    <column name="unitarea"  
		length="80"               
    />
</property>	
    <property name="unitshortname" type="string"  not-null="false">
    <column name="unitshortname"  
		length="80"               
    />
</property>	
    <property name="scheduleunit" type="string"  not-null="false">
    <column name="scheduleunit"  
		length="80"               
    />

</property>	
    <property name="iscanedit" type="string"  not-null="false">
    <column name="iscanedit"  
		length="2"               
    />
</property>	
    <property name="maintenanceunitid" type="string"  not-null="false">
    <column name="maintenanceunitid"  
		length="80"               
    />
</property>	
    <property name="datalocation" type="string"  not-null="false">
    <column name="datalocation"  
		length="80"               
    />
</property>	
    <property name="eshirtname" type="string"  not-null="false">
    <column name="eshirtname"  
		length="8"               
    />
</property>	
  
</class>

</hibernate-mapping>

