<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.techstar.dmis.entity">
	<class name="Mequivalentload" table="DMIS_MEQUIVALENTLOAD" dynamic-insert="true" dynamic-update="true" optimistic-lock="version">
    <id name="fid" type="string">
      <column name="fid" sql-type="varchar(40)" not-null="true"/>
      <generator class="assigned"/>
    </id>

 	<version name="version" column="VERSION" unsaved-value="undefined"/>
  
   <property name="fname" type="string"  not-null="false">
    <column name="fname"  
		length="40"               
    />
</property>	
    <property name="fdescription" type="string"  not-null="false">
    <column name="fdescription"  
		length="40"               
    />
</property>	
    <property name="faliasname" type="string"  not-null="false">
    <column name="faliasname"  
		length="40"               
    />
</property>	
    <property name="fterminals" type="string"  not-null="false">
    <column name="fterminals"  
		length="500"               
    />
</property>	
    <property name="fmemberof_efquipmentcontainer" type="string"  not-null="false">
    <column name="fmemberof_efquipmentcontainer"  
		length="40"               
    />
</property>	
  
</class>

</hibernate-mapping>

